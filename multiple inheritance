#include<iostream>
using namespace std;
class base1
{
	public:
		a()
		{
			cout<<"a"<<endl;
		}
};
class base2
{
	public:
		b()
		{
			cout<<"b"<<endl;
		}
};
class derived:public base1,public base2
{
	public:
		c()
		{
			cout<<"c"<<endl;
		}
};
int main()
{
	derived obj;
	obj.a();
	obj.b();
	obj.c();
	return 0;
}
