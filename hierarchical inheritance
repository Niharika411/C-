#include<iostream>
using namespace std;
class base
{
	public:
		a()
		{
			cout<<"a"<<endl;
		}
};
class derived1:public base
{
	public:
		b()
		{
			cout<<"b"<<endl;
		}
};
class derived2:public base
{
	public:
		c()
		{
			cout<<"c"<<endl;
		}
};
class derived3:public base
{
	public:
		d()
		{
			cout<<"d"<<endl;
		}
};
int main()
{
derived1 obj1;
derived2 obj2;
derived3 obj3;
obj1.a();
obj1.b();
obj2.a();
obj2.c();
obj3.a();
obj3.d();
return 0;
}
