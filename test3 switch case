#include<iostream>
int main()
{
	int a,b,c,d;
	std::cout<<"enter the value of a:";
	std::cin>>a;
	std::cout<<"enter the value of b:";
	std::cin>>b;
	std::cout<<"enter your case";
	std::cin>>c;
	switch(c)
	{
		case 1:
			d=a+b;
			std::cout<<d;
			break;
		case 2:
		    d=a-b;
			std::cout<<d;
			break;
		case 3:
		    d=a*b;
			std::cout<<d;
			break;
		case 4:
			d=a/b;
			std::cout<<d;
			break;
		case 5:
			d=a%b;
			std::cout<<d;
			break;	
		default:
		std::cout<<"invalid choice";
		break;		
	}
	return 0;
}
