#include<iostream>
using namespace std;
class Degree
{
	public:
		void get_degree()
	{
		cout<<" I got a Degree"<<endl;
	
	}
};
class Undergraduate:public Degree
{
	public:
		graduate()
		{
			cout<<" I am a Undergraduate"<<endl;
		}
};
class Postgraduate:public Degree
{
	public:
		void graduate()
	{
		cout<<" I am a Postgraduate"<<endl;
	}
};
int main()
{
	Undergraduate obj1;
	Postgraduate obj2;
	obj1.get_degree();
	obj1.graduate();
	obj2.get_degree();
	obj2.graduate();
	return 0;
}
